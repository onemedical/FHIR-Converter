{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Goal",
        "id":"{{ID}}",

        "identifier":
        [
        	{{#each (toArray goalEntry.id)}}
            	{{>DataType/Identifier.hbs id=this}},
            {{/each}}
        ],

        "description": {
          "coding": [
            {
              "system": "http://loinc.org",
              "code": "61146-7"
            }
          ],
          "text": "Get rid of intermittent fever that is occurring every few weeks.Need to gain more energy to do regular activities"
        },
        "lifecycleStatus":"{{goalEntry.statusCode.code}}",

        "target": [
            {
                "dueDate": "{{addHyphensDate goalEntry.effectiveTime.value}}",
            }
        ],
    },
    "request":{
        "method":"PUT",
        "url":"Goal/{{ID}}",
    },
},
